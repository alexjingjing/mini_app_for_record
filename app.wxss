/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
}

.table {
  border: 0px solid darkgray;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}

.td {
  width: 40%;
  justify-content: center;
  text-align: center;
}

.bg-w {
  background: snow;
}

.bg-g {
  background: #e6f3f9;
}

.th {
  width: 40%;
  justify-content: center;
  background: #3fa2e0;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}

.tr-bubble {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}

@media (-webkit-min-device-pixel-ratio: 2),(min-device-pixel-ratio: 2) {
  .th-bubble {
    width: 40%;
    justify-content: center;
    background: #fff;
    color: #000;
    display: flex;
    align-items: center;
    margin-bottom: 12rpx;
  }
}

@media (-webkit-min-device-pixel-ratio: 3),(min-device-pixel-ratio: 3) {
  .th-bubble {
    width: 40%;
    justify-content: center;
    background: #fff;
    color: #000;
    display: flex;
    align-items: center;
    margin-bottom: 16rpx;
  }
}
