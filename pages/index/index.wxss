/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction:column;
  align-items: center;
}

.row-center {
  display: flex;
flex-direction: column;
justify-content: center;
align-items: center;
}

.bold-style {
  font-weight: bold;
}

.bubble-title {
  margin-top: 24rpx;
  margin-left: 60rpx;
  font-size: 11pt;
  color: white
}

.bubble-content {
  margin-top: 10rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
}

.bubble-sub-title {
  margin-top: 4rpx;
  margin-left: 60rpx;
  font-size: 7pt;
  color: white
}

.bubble-back {
  width: 710rpx;
  height: 300rpx;
  background-size: contain;
  background-image: url('data:image/png;base64,');
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.stock-list {
  padding: 4px 6px 48px;
}

.checkbox {
  display: flex;
  flex-direction:row;
}

.margin-button {
  margin: 20rpx;
}

.panel-text {
  border-left-style: solid;
  border-left-width: 4px;
  border-left-color: rgb(112, 112, 122);
  margin: 8rpx;
  padding-left: 8px;
}
